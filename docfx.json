{
    "metadata": [
        {
            "src": [
                {
                    "src": "..",
                    "files": [
                        "Tamato.GaifaGoap.csproj"
                    ]
                }
            ],
            "globalNamespaceId": "Global",
            "filter": "filterConfig.yml",
            "dest": "api"
        }
    ],
    "build": {
        "globalMetadata": {
            "_appTitle": "Gaifa API documentation",
            "_appFooter": "Â© 2021 Mahidol University All Rights Reserved | Gaifa API documentation",
            "_enableSearch": true,
            "_disableContribution": true
        },
        "content": [
            {
                "files": [
                    "toc.yml",
                    "index.md"
                ]
            },
            {
                "src": "api",
                "files": [
                    "*.yml"
                ],
                "dest": "api"
            }
        ],
        "overwrite": [
            {
                "src": "..",
                "files": [
                    "Assets/Scripts/**/*.md"
                ]
            }
        ],
        "sitemap": {
            "changefreq": "weekly",
            "fileOptions": {
                "api/*": {
                    "changefreq": "daily"
                }
            }
        },
        "xref": [
            "https://normanderwan.github.io/UnityXrefMaps/xrefmap.yml"
        ],
        "xrefService": [
            "https://xref.docs.microsoft.com/query?uid={uid}"
        ],
        "dest": "_site",
        "template": ["default", "templates/default"]
    }
}